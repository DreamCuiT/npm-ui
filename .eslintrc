{
  "globals": {
    "ga": true,
    "chrome": true
  },
  "plugins": ["html", "json"],
  "extends": ["plugin:vue/essential"],
  "rules": {
    "no-restricted-globals": ["error", "event", "fdescribe"],
    "indent": "off",
    "no-unused-vars": "off"
  },
  "env": { "es6": true },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "parser": "babel-eslint"
  }
}
